{
  "internalConfig": {
    "options": {
      "mode": 2,
      "batchSize": 1,
      "pollFrequency": 10,
      "recursiveTimeout": 100,
      "messageCharset": "utf8",
      "messageAsJSON": true,
      "sync": true,
      "consumeTimeout": 1000
    },
    "rdkafkaConf": {
      "client.id": "ml-con-notification-event",
      "group.id": "ml-group-notification-event",
      "metadata.broker.list": "localhost:9092",
      "socket.keepalive.enable": true
    },
    "topicConf": {
      "auto.offset.reset": "earliest"
    }
  }
}
